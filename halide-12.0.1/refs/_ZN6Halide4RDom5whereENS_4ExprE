<use f='halide/apps/hannk/halide/pool_generator.cpp' l='48' u='c' c='_ZN5hannk11AveragePool8generateEv'/>
<use f='halide/apps/hannk/halide/pool_generator.cpp' l='133' u='c' c='_ZN5hannk7MaxPool8generateEv'/>
