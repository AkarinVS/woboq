<dec f='llvm/lldb/include/lldb/Target/ThreadList.h' l='124' type='bool lldb_private::ThreadList::WillResume()'/>
<doc f='llvm/lldb/include/lldb/Target/ThreadList.h' l='113'>/// The thread list asks tells all the threads it is about to resume.
  /// If a thread can &quot;resume&quot; without having to resume the target, it
  /// will return false for WillResume, and then the process will not be
  /// restarted.
  ///
  /// \return
  ///    \b true instructs the process to resume normally,
  ///    \b false means start &amp; stopped events will be generated, but
  ///    the process will not actually run.  The thread must then return
  ///    the correct StopInfo when asked.
  ///</doc>
<use f='llvm/lldb/source/Target/Process.cpp' l='2994' u='c' c='_ZN12lldb_private7Process13PrivateResumeEv'/>
<def f='llvm/lldb/source/Target/ThreadList.cpp' l='484' ll='617' type='bool lldb_private::ThreadList::WillResume()'/>
