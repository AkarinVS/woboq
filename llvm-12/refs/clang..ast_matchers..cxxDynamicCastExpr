<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='2507'>/// Matches a dynamic_cast expression.
///
/// Example:
///   cxxDynamicCastExpr()
/// matches
///   dynamic_cast&lt;D*&gt;(&amp;b);
/// in
/// \code
///   struct B { virtual ~B() {} }; struct D : B {};
///   B b;
///   D* p = dynamic_cast&lt;D*&gt;(&amp;b);
/// \endcode</doc>
