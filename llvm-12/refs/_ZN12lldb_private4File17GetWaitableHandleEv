<dec f='llvm/lldb/include/lldb/Host/File.h' l='125' type='lldb_private::IOObject::WaitableHandle lldb_private::File::GetWaitableHandle()'/>
<doc f='llvm/lldb/include/lldb/Host/File.h' l='116'>/// Get a handle that can be used for OS polling interfaces, such
  /// as WaitForMultipleObjects, select, or epoll.   This may return
  /// IOObject::kInvalidHandleValue if none is available.   This will
  /// generally be the same as the file descriptor, this function
  /// is not interchangeable with GetDescriptor().   A WaitableHandle
  /// must only be used for polling, not actual I/O.
  ///
  /// \return
  ///     a valid handle or IOObject::kInvalidHandleValue</doc>
<inh f='llvm/lldb/include/lldb/Utility/IOObject.h' l='42' c='_ZN12lldb_private8IOObject17GetWaitableHandleEv'/>
