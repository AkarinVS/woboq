<mbr r='isl_tarjan_graph::sp' o='192' t='int'/>
<mbr r='isl_tarjan_graph::index' o='224' t='int'/>
<mbr r='isl_tarjan_graph::order' o='256' t='int *'/>
<mbr r='isl_tarjan_graph::op' o='320' t='int'/>
