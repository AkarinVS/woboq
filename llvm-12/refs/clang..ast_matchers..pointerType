<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='6530'>/// Matches pointer types, but does not match Objective-C object pointer
/// types.
///
/// Given
/// \code
///   int *a;
///   int &amp;b = *a;
///   int c = 5;
///
///   @interface Foo
///   @end
///   Foo *f;
/// \endcode
/// pointerType()
///   matches &quot;int *a&quot;, but does not match &quot;Foo *f&quot;.</doc>
