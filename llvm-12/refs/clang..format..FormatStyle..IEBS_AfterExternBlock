<doc f='llvm/clang/include/clang/Format/Format.h' l='2137'>/// Backwards compatible with AfterExternBlock&apos;s indenting.
    /// \code
    ///    IndentExternBlock: AfterExternBlock
    ///    BraceWrapping.AfterExternBlock: true
    ///    extern &quot;C&quot;
    ///    {
    ///        void foo();
    ///    }
    /// \endcode
    ///
    /// \code
    ///    IndentExternBlock: AfterExternBlock
    ///    BraceWrapping.AfterExternBlock: false
    ///    extern &quot;C&quot; {
    ///    void foo();
    ///    }
    /// \endcode</doc>
