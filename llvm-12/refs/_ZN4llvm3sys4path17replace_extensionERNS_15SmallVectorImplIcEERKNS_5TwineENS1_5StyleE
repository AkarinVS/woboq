<dec f='llvm/llvm/include/llvm/Support/Path.h' l='148' type='void llvm::sys::path::replace_extension(SmallVectorImpl&lt;char&gt; &amp; path, const llvm::Twine &amp; extension, llvm::sys::path::Style style = Style::native)'/>
<doc f='llvm/llvm/include/llvm/Support/Path.h' l='136'>/// Replace the file extension of \a path with \a extension.
///
/// @code
///   ./filename.cpp =&gt; ./filename.extension
///   ./filename     =&gt; ./filename.extension
///   ./             =&gt; ./.extension
/// @endcode
///
/// @param path A path that has its extension replaced with \a extension.
/// @param extension The extension to be added. It may be empty. It may also
///                  optionally start with a &apos;.&apos;, if it does not, one will be
///                  prepended.</doc>
