<def f='llvm/llvm/include/llvm/ADT/FunctionExtras.h' l='172' ll='174' type='void * llvm::detail::UniqueFunctionBase::getCalleePtr() const'/>
<doc f='llvm/llvm/include/llvm/ADT/FunctionExtras.h' l='166'>// These three functions are only const in the narrow sense. They return
  // mutable pointers to function state.
  // This allows unique_function&lt;T const&gt;::operator() to be const, even if the
  // underlying functor may be internally mutable.
  //
  // const callers must ensure they&apos;re only used in const-correct ways.</doc>
