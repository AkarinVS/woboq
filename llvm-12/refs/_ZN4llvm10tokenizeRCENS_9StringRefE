<dec f='llvm/llvm/tools/llvm-rc/ResourceScriptToken.h' l='77' type='Expected&lt;std::vector&lt;RCToken&gt; &gt; llvm::tokenizeRC(llvm::StringRef Input)'/>
<doc f='llvm/llvm/tools/llvm-rc/ResourceScriptToken.h' l='68'>// Tokenize Input.
// In case no error occurred, the return value contains
//   tokens in order they were in the input file.
// In case of any error, the return value contains
//   a textual representation of error.
//
// Tokens returned by this function hold only references to the parts
// of the Input. Memory buffer containing Input cannot be freed,
// modified or reallocated.</doc>
<def f='llvm/llvm/tools/llvm-rc/ResourceScriptToken.cpp' l='363' ll='365' type='Expected&lt;std::vector&lt;RCToken&gt; &gt; llvm::tokenizeRC(llvm::StringRef Input)'/>
<use f='llvm/llvm/tools/llvm-rc/llvm-rc.cpp' l='121' u='c' c='main'/>
