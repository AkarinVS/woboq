<mbr r='pet_scop::context_value' o='128' t='isl_set *'/>
<mbr r='pet_scop::schedule' o='192' t='isl_schedule *'/>
<mbr r='pet_scop::n_type' o='256' t='int'/>
<mbr r='pet_scop::types' o='320' t='struct pet_type **'/>
<mbr r='pet_scop::n_array' o='384' t='int'/>
<mbr r='pet_scop::arrays' o='448' t='struct pet_array **'/>
<mbr r='pet_scop::n_stmt' o='512' t='int'/>
<mbr r='pet_scop::stmts' o='576' t='struct pet_stmt **'/>
