<doc f='llvm/clang/include/clang/Format/Format.h' l='1408'>/// Only wrap braces after a multi-line control statement.
    /// \code
    ///   if (foo &amp;&amp; bar &amp;&amp;
    ///       baz)
    ///   {
    ///     quux();
    ///   }
    ///   while (foo || bar) {
    ///   }
    /// \endcode</doc>
