<dec f='llvm/llvm/include/llvm/Support/PrettyStackTrace.h' l='38' type='void llvm::EnablePrettyStackTraceOnSigInfoForThisThread(bool ShouldEnable = true)'/>
<doc f='llvm/llvm/include/llvm/Support/PrettyStackTrace.h' l='29'>/// Enables (or disables) dumping a &quot;pretty&quot; stack trace when the user sends
  /// SIGINFO or SIGUSR1 to the current process.
  ///
  /// This is a per-thread decision so that a program can choose to print stack
  /// traces only on a primary thread, or on all threads that use
  /// PrettyStackTraceEntry.
  ///
  /// \see EnablePrettyStackTrace
  /// \see PrettyStackTraceEntry</doc>
<def f='llvm/llvm/lib/Support/PrettyStackTrace.cpp' l='285' ll='305' type='void llvm::EnablePrettyStackTraceOnSigInfoForThisThread(bool ShouldEnable = true)'/>
