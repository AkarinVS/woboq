<dec f='llvm/lldb/include/lldb/Target/ExecutionContext.h' l='197' type='void lldb_private::ExecutionContextRef::SetFrameSP(const lldb::StackFrameSP &amp; frame_sp)'/>
<doc f='llvm/lldb/include/lldb/Target/ExecutionContext.h' l='184'>/// Set accessor that creates a weak reference to the frame referenced in \a
  /// frame_sp.
  ///
  /// If \a frame_sp is valid this object will create a weak reference to that
  /// object, otherwise any previous frame weak reference contained in this
  /// object will be reset.
  ///
  /// Only the weak reference to the frame will be updated, no other weak
  /// references will be modified. If you want this execution context to make
  /// a weak reference to the frame&apos;s thread, process and target, use the
  /// ExecutionContextRef::SetContext() functions.
  ///
  /// \see ExecutionContextRef::SetContext(const lldb::StackFrameSP &amp;)</doc>
<use f='llvm/lldb/source/API/SBExecutionContext.cpp' l='65' u='c' c='_ZN4lldb18SBExecutionContextC1ERKNS_7SBFrameE'/>
<use f='llvm/lldb/source/API/SBFrame.cpp' l='89' u='c' c='_ZN4lldb7SBFrame10SetFrameSPERKSt10shared_ptrIN12lldb_private10StackFrameEE'/>
