<dec f='llvm/llvm/include/llvm/Analysis/LoopInfo.h' l='836' type='void llvm::Loop::setLoopID(llvm::MDNode * LoopID) const'/>
<doc f='llvm/llvm/include/llvm/Analysis/LoopInfo.h' l='829'>/// Set the llvm.loop loop id metadata for this loop.
  ///
  /// The LoopID metadata node will be added to each terminator instruction in
  /// the loop that branches to the loop header.
  ///
  /// The LoopID metadata node should have one or more operands and the first
  /// operand should be the node itself.</doc>
