<doc f='llvm/clang/include/clang/Format/Format.h' l='2791'>/// Put a space before opening parentheses only if the parentheses are not
    /// empty i.e. &apos;()&apos;
    /// \code
    ///   void() {
    ///     if (true) {
    ///       f();
    ///       g (x, y, z);
    ///     }
    ///   }
    /// \endcode</doc>
