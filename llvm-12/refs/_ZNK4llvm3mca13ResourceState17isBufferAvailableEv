<dec f='llvm/llvm/include/llvm/MCA/HardwareUnits/ResourceManager.h' l='262' type='llvm::mca::ResourceStateEvent llvm::mca::ResourceState::isBufferAvailable() const'/>
<doc f='llvm/llvm/include/llvm/MCA/HardwareUnits/ResourceManager.h' l='253'>/// Checks if there is an available slot in the resource buffer.
  ///
  /// Returns RS_BUFFER_AVAILABLE if this is not a buffered resource, or if
  /// there is a slot available.
  ///
  /// Returns RS_RESERVED if this buffered resource is a dispatch hazard, and it
  /// is reserved.
  ///
  /// Returns RS_BUFFER_UNAVAILABLE if there are no available slots.</doc>
<def f='llvm/llvm/lib/MCA/HardwareUnits/ResourceManager.cpp' l='85' ll='91' type='llvm::mca::ResourceStateEvent llvm::mca::ResourceState::isBufferAvailable() const'/>
