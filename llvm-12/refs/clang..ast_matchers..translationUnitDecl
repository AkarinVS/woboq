<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='168'>/// Matches the top declaration context.
///
/// Given
/// \code
///   int X;
///   namespace NS {
///   int Y;
///   }  // namespace NS
/// \endcode
/// decl(hasDeclContext(translationUnitDecl()))
///   matches &quot;int X&quot;, but not &quot;int Y&quot;.</doc>
