<doc f='llvm/clang/include/clang/Format/Format.h' l='2174'>/// Indent the requires clause in a template
  /// \code
  ///    true:
  ///    template &lt;typename It&gt;
  ///      requires Iterator&lt;It&gt;
  ///    void sort(It begin, It end) {
  ///      //....
  ///    }
  ///
  ///    false:
  ///    template &lt;typename It&gt;
  ///    requires Iterator&lt;It&gt;
  ///    void sort(It begin, It end) {
  ///      //....
  ///    }
  /// \endcode</doc>
