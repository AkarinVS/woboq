<doc f='llvm/clang/include/clang/Format/Format.h' l='570'>/// Only merge functions defined inside a class. Same as &quot;inline&quot;,
    /// except it does not implies &quot;empty&quot;: i.e. top level empty functions
    /// are not merged either.
    /// \code
    ///   class Foo {
    ///     void f() { foo(); }
    ///   };
    ///   void f() {
    ///     foo();
    ///   }
    ///   void f() {
    ///   }
    /// \endcode</doc>
