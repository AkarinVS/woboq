<fun r='_ZN4llvm27ModuleToFunctionPassAdaptor3runERNS_6ModuleERNS_15AnalysisManagerIS1_JEEE'/>
<fun r='_ZN4llvm27ModuleToFunctionPassAdaptor10isRequiredEv'/>
