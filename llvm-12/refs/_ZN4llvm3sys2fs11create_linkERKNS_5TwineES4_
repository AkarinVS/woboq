<dec f='llvm/llvm/include/llvm/Support/FileSystem.h' l='329' type='std::error_code llvm::sys::fs::create_link(const llvm::Twine &amp; to, const llvm::Twine &amp; from)'/>
<doc f='llvm/llvm/include/llvm/Support/FileSystem.h' l='318'>/// Create a link from \a from to \a to.
///
/// The link may be a soft or a hard link, depending on the platform. The caller
/// may not assume which one. Currently on windows it creates a hard link since
/// soft links require extra privileges. On unix, it creates a soft link since
/// hard links don&apos;t work on SMB file systems.
///
/// @param to The path to hard link to.
/// @param from The path to hard link from. This is created.
/// @returns errc::success if the link was created, otherwise a platform
/// specific error_code.</doc>
<use f='llvm/llvm/include/llvm/Testing/Support/SupportHelpers.h' l='180' u='c' c='_ZN4llvm8unittest8TempLinkC1ENS_9StringRefES2_'/>
<use f='llvm/llvm/lib/Support/LockFileManager.cpp' l='220' u='c' c='_ZN4llvm15LockFileManagerC1ENS_9StringRefE'/>
