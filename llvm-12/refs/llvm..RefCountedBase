<fun r='_ZN4llvm14RefCountedBaseaSERKNS_14RefCountedBaseIT_EE'/>
<fun r='_ZN4llvm14RefCountedBaseD1Ev'/>
<fun r='_ZNK4llvm14RefCountedBase6RetainEv'/>
<fun r='_ZNK4llvm14RefCountedBase7ReleaseEv'/>
