<mbr r='llvm::IRSimilarity::IRSimilarityCandidate::ValueToNumber' o='192' t='DenseMap&lt;llvm::Value *, unsigned int&gt;'/>
<mbr r='llvm::IRSimilarity::IRSimilarityCandidate::NumberToValue' o='384' t='DenseMap&lt;unsigned int, llvm::Value *&gt;'/>
