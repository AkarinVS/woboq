<dec f='llvm/llvm/include/llvm/CodeGen/TargetFrameLowering.h' l='344' type='void llvm::TargetFrameLowering::determineCalleeSaves(llvm::MachineFunction &amp; MF, llvm::BitVector &amp; SavedRegs, llvm::RegScavenger * RS = nullptr) const'/>
<doc f='llvm/llvm/include/llvm/CodeGen/TargetFrameLowering.h' l='334'>/// This method determines which of the registers reported by
  /// TargetRegisterInfo::getCalleeSavedRegs() should actually get saved.
  /// The default implementation checks populates the \p SavedRegs bitset with
  /// all registers which are modified in the function, targets may override
  /// this function to save additional registers.
  /// This method also sets up the register scavenger ensuring there is a free
  /// register or a frameindex available.
  /// This method should not be called by any passes outside of PEI, because
  /// it may change state passed in by \p MF and \p RS. The preferred
  /// interface outside PEI is getCalleeSaves.</doc>
<use f='llvm/llvm/lib/CodeGen/PrologEpilogInserter.cpp' l='609' u='c' c='_ZN12_GLOBAL__N_13PEI20spillCalleeSavedRegsERN4llvm15MachineFunctionE'/>
<use f='llvm/llvm/lib/CodeGen/RegisterScavenging.cpp' l='784' u='c' c='_ZN12_GLOBAL__N_113ScavengerTest20runOnMachineFunctionERN4llvm15MachineFunctionE'/>
<use f='llvm/llvm/lib/CodeGen/ShrinkWrap.cpp' l='171' u='c' c='_ZNK12_GLOBAL__N_110ShrinkWrap14getCurrentCSRsEPN4llvm12RegScavengerE'/>
