<dec f='llvm/lldb/include/lldb/API/SBModule.h' l='63' type='lldb::SBFileSpec lldb::SBModule::GetPlatformFileSpec() const'/>
<use f='llvm/build/tools/lldb/bindings/python/LLDBWrapPython.cpp' l='38762' u='c' c='_ZL34_wrap_SBModule_GetPlatformFileSpecP7_objectS0_'/>
<doc f='llvm/lldb/include/lldb/API/SBModule.h' l='50'>/// Get accessor for the module platform file specification.
  ///
  /// Platform file refers to the path of the module as it is known on
  /// the remote system on which it is being debugged. For local
  /// debugging this is always the same as Module::GetFileSpec(). But
  /// remote debugging might mention a file &apos;/usr/lib/liba.dylib&apos;
  /// which might be locally downloaded and cached. In this case the
  /// platform file could be something like:
  /// &apos;/tmp/lldb/platform-cache/remote.host.computer/usr/lib/liba.dylib&apos;
  /// The file could also be cached in a local developer kit directory.
  ///
  /// \return
  ///     A const reference to the file specification object.</doc>
<def f='llvm/lldb/source/API/SBModule.cpp' l='107' ll='117' type='lldb::SBFileSpec lldb::SBModule::GetPlatformFileSpec() const'/>
<use f='llvm/lldb/source/API/SBModule.cpp' l='109' u='a' c='_ZNK4lldb8SBModule19GetPlatformFileSpecEv'/>
<use f='llvm/lldb/source/API/SBModule.cpp' l='109' u='a' c='_ZNK4lldb8SBModule19GetPlatformFileSpecEv'/>
