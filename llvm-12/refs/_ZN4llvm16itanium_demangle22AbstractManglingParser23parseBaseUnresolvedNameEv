<def f='llvm/llvm/include/llvm/Demangle/ItaniumDemangle.h' l='3303' ll='3323' type='llvm::itanium_demangle::Node * llvm::itanium_demangle::AbstractManglingParser::parseBaseUnresolvedName()'/>
<doc f='llvm/llvm/include/llvm/Demangle/ItaniumDemangle.h' l='3296'>// &lt;base-unresolved-name&gt; ::= &lt;simple-id&gt;                                # unresolved name
//          extension     ::= &lt;operator-name&gt;                            # unresolved operator-function-id
//          extension     ::= &lt;operator-name&gt; &lt;template-args&gt;            # unresolved operator template-id
//                        ::= on &lt;operator-name&gt;                         # unresolved operator-function-id
//                        ::= on &lt;operator-name&gt; &lt;template-args&gt;         # unresolved operator template-id
//                        ::= dn &lt;destructor-name&gt;                       # destructor or pseudo-destructor;
//                                                                         # e.g. ~X or ~X&lt;N-1&gt;</doc>
