<dec f='llvm/lldb/source/Plugins/ExpressionParser/Clang/ClangExpressionVariable.h' l='114' type='bool lldb_private::ClangExpressionVariable::PointValueAtData(lldb_private::Value &amp; value, lldb_private::ExecutionContext * exe_ctx)'/>
<doc f='llvm/lldb/source/Plugins/ExpressionParser/Clang/ClangExpressionVariable.h' l='101'>/// If the variable contains its own data, make a Value point at it. If \a
  /// exe_ctx in not NULL, the value will be resolved in with that execution
  /// context.
  ///
  /// \param[in] value
  ///     The value to point at the data.
  ///
  /// \param[in] exe_ctx
  ///     The execution context to use to resolve \a value.
  ///
  /// \return
  ///     True on success; false otherwise (in particular, if this variable
  ///     does not contain its own data).</doc>
