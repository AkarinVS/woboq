<dec f='llvm/llvm/tools/llvm-cfi-verify/lib/FileAnalysis.h' l='130' type='const llvm::cfi_verify::FileAnalysis::Instr * llvm::cfi_verify::FileAnalysis::getDefiniteNextInstruction(const llvm::cfi_verify::FileAnalysis::Instr &amp; InstrMeta) const'/>
<def f='llvm/llvm/tools/llvm-cfi-verify/lib/FileAnalysis.cpp' l='199' ll='228' type='const Instr * llvm::cfi_verify::FileAnalysis::getDefiniteNextInstruction(const llvm::cfi_verify::FileAnalysis::Instr &amp; InstrMeta) const'/>
<doc f='llvm/llvm/tools/llvm-cfi-verify/lib/FileAnalysis.h' l='123'>// Returns the definitive next instruction. This is different from the next
  // instruction sequentially as it will follow unconditional branches (assuming
  // they can be resolved at compile time, i.e. not indirect). This method
  // returns nullptr if the provided instruction does not transfer control flow
  // to exactly one instruction that is known deterministically at compile time.
  // Also returns nullptr if the deterministic target does not exist in this
  // file.</doc>
