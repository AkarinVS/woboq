<doc f='llvm/clang/include/clang/AST/DeclOpenMP.h' l='98'>/// This represents &apos;#pragma omp threadprivate ...&apos; directive.
/// For example, in the following, both &apos;a&apos; and &apos;A::b&apos; are threadprivate:
///
/// \code
/// int a;
/// #pragma omp threadprivate(a)
/// struct A {
///   static int b;
/// #pragma omp threadprivate(b)
/// };
/// \endcode
///</doc>
