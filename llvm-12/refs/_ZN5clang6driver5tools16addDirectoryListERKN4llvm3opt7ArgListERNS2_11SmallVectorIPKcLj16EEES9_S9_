<dec f='llvm/clang/lib/Driver/ToolChains/CommonArgs.h' l='101' type='void clang::driver::tools::addDirectoryList(const llvm::opt::ArgList &amp; Args, llvm::opt::ArgStringList &amp; CmdArgs, const char * ArgName, const char * EnvVar)'/>
<doc f='llvm/clang/lib/Driver/ToolChains/CommonArgs.h' l='96'>/// \p EnvVar is split by system delimiter for environment variables.
/// If \p ArgName is &quot;-I&quot;, &quot;-L&quot;, or an empty string, each entry from \p EnvVar
/// is prefixed by \p ArgName then added to \p Args. Otherwise, for each
/// entry of \p EnvVar, \p ArgName is added to \p Args first, then the entry
/// itself is added.</doc>
