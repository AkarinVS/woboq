<dec f='llvm/lldb/include/lldb/Utility/Connection.h' l='142' type='size_t lldb_private::Connection::Write(const void * dst, size_t dst_len, lldb::ConnectionStatus &amp; status, lldb_private::Status * error_ptr)'/>
<doc f='llvm/lldb/include/lldb/Utility/Connection.h' l='122'>/// The actual write function that attempts to write to the communications
  /// protocol.
  ///
  /// Subclasses must override this function.
  ///
  /// \param[in] dst
  ///     A destination buffer that must be at least \a dst_len bytes
  ///     long.
  ///
  /// \param[in] dst_len
  ///     The number of bytes to attempt to write, and also the
  ///     number of bytes are currently available in \a dst.
  ///
  /// \param[out] error_ptr
  ///     A pointer to an error object that should be given an
  ///     appropriate error value if this method returns zero. This
  ///     value can be NULL if the error value should be ignored.
  ///
  /// \return
  ///     The number of bytes actually Written.</doc>
