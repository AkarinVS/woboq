<dec f='llvm/llvm/lib/Target/AArch64/AArch64ISelLowering.h' l='787' type='bool llvm::AArch64TargetLowering::mergeStoresAfterLegalization(llvm::EVT VT) const'/>
<doc f='llvm/llvm/lib/Target/AArch64/AArch64ISelLowering.h' l='781'>/// SVE code generation for fixed length vectors does not custom lower
  /// BUILD_VECTOR. This makes BUILD_VECTOR legalisation a source of stores to
  /// merge. However, merging them creates a BUILD_VECTOR that is just as
  /// illegal as the original, thus leading to an infinite legalisation loop.
  /// NOTE: Once BUILD_VECTOR is legal or can be custom lowered for all legal
  /// vector types this override can be removed.</doc>
<inh f='llvm/llvm/include/llvm/CodeGen/TargetLowering.h' l='583' c='_ZNK4llvm18TargetLoweringBase28mergeStoresAfterLegalizationENS_3EVTE'/>
<def f='llvm/llvm/lib/Target/AArch64/AArch64ISelLowering.cpp' l='4451' ll='4453' type='bool llvm::AArch64TargetLowering::mergeStoresAfterLegalization(llvm::EVT VT) const'/>
