<fun r='_ZN5polly13ScopAnnotator8pushLoopEPN4llvm4LoopEb'/>
<fun r='_ZN5polly13ScopAnnotator7popLoopEb'/>
<fun r='_ZN5polly13ScopAnnotator8annotateEPN4llvm11InstructionE'/>
<fun r='_ZNK5polly13ScopAnnotator17annotateLoopLatchEPN4llvm10BranchInstEPNS1_4LoopEbb'/>
<fun r='_ZN5polly13ScopAnnotator24addAlternativeAliasBasesERN4llvm8DenseMapINS1_11AssertingVHINS1_5ValueEEES5_NS1_12DenseMapInfoIS5_EENS1_6detail12DenseMapPairIS5_S5_EEEE'/>
<fun r='_ZN5polly13ScopAnnotator26resetAlternativeAliasBasesEv'/>
