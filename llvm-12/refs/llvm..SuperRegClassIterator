<fun r='_ZNK4llvm21SuperRegClassIterator7isValidEv'/>
<fun r='_ZNK4llvm21SuperRegClassIterator9getSubRegEv'/>
<fun r='_ZNK4llvm21SuperRegClassIterator7getMaskEv'/>
<fun r='_ZN4llvm21SuperRegClassIteratorppEv'/>
