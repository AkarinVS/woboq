<fun r='_ZNK5clang17DiagnosticBuilder5ClearEv'/>
<fun r='_ZNK5clang17DiagnosticBuilder8isActiveEv'/>
<fun r='_ZN5clang17DiagnosticBuilder4EmitEv'/>
<fun r='_ZN5clang17DiagnosticBuilderC1ERKS0_'/>
<fun r='_ZNK5clang17DiagnosticBuilderlsERKT_'/>
<fun r='_ZNK5clang17DiagnosticBuilderlsEOT_'/>
<fun r='_ZN5clang17DiagnosticBuilderaSERKS0_'/>
<fun r='_ZN5clang17DiagnosticBuilderD1Ev'/>
<fun r='_ZNK5clang17DiagnosticBuilder12setForceEmitEv'/>
<fun r='_ZNK5clang17DiagnosticBuilder12addFlagValueEN4llvm9StringRefE'/>
