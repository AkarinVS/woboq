<def f='llvm/llvm/include/llvm/Demangle/ItaniumDemangle.h' l='3076' ll='3120' type='llvm::itanium_demangle::Node * llvm::itanium_demangle::AbstractManglingParser::parseCtorDtorName(llvm::itanium_demangle::Node *&amp; SoFar, llvm::itanium_demangle::AbstractManglingParser::NameState * State)'/>
<doc f='llvm/llvm/include/llvm/Demangle/ItaniumDemangle.h' l='3066'>// &lt;ctor-dtor-name&gt; ::= C1  # complete object constructor
//                  ::= C2  # base object constructor
//                  ::= C3  # complete object allocating constructor
//   extension      ::= C4  # gcc old-style &quot;[unified]&quot; constructor
//   extension      ::= C5  # the COMDAT used for ctors
//                  ::= D0  # deleting destructor
//                  ::= D1  # complete object destructor
//                  ::= D2  # base object destructor
//   extension      ::= D4  # gcc old-style &quot;[unified]&quot; destructor
//   extension      ::= D5  # the COMDAT used for dtors</doc>
