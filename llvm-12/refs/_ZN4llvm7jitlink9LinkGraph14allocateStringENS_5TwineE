<def f='llvm/llvm/include/llvm/ExecutionEngine/JITLink/JITLink.h' l='823' ll='829' type='llvm::StringRef llvm::jitlink::LinkGraph::allocateString(llvm::Twine Source)'/>
<doc f='llvm/llvm/include/llvm/ExecutionEngine/JITLink/JITLink.h' l='816'>/// Allocate a copy of the given string using the LinkGraph&apos;s allocator.
  /// This can be useful when renaming symbols or adding new content to the
  /// graph.
  ///
  /// Note: This Twine-based overload requires an extra string copy and an
  /// extra heap allocation for large strings. The StringRef overload should
  /// be preferred where possible.</doc>
