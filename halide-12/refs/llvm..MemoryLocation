<fun r='_ZN4llvm14MemoryLocation3getEPKNS_11InstructionE'/>
<fun r='_ZN4llvm14MemoryLocation9getOrNoneEPKNS_11InstructionE'/>
<fun r='_ZN4llvm14MemoryLocation12getForSourceEPKNS_15MemTransferInstE'/>
<fun r='_ZN4llvm14MemoryLocation12getForSourceEPKNS_21AtomicMemTransferInstE'/>
<fun r='_ZN4llvm14MemoryLocation12getForSourceEPKNS_18AnyMemTransferInstE'/>
