<fun r='_ZN4llvm14WeakTrackingVHaSERKNS_15ValueHandleBaseE'/>
<fun r='_ZNK4llvm14WeakTrackingVHcvPNS_5ValueEEv'/>
<fun r='_ZNK4llvm14WeakTrackingVH18pointsToAliveValueEv'/>
